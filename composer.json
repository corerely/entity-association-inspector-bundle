{
    "name": "corerely/entity-association-inspector-bundle",
    "description": "This bundle provide tool for inspection relations between entities.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrii Oprysko",
            "email": "anjey.link@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Corerely\\EntityAssociationInspectorBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Corerely\\EntityAssociationInspectorBundle\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "symfony/http-kernel": "^5.2",
        "symfony/dependency-injection": "^5.2",
        "symfony/config": "^5.2",
        "doctrine/orm": "^2.8",
        "doctrine/doctrine-bundle": "^2.3"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.2",
        "symfony/framework-bundle": "^5.2"
    }
}
