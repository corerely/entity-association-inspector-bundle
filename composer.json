{
    "name": "corerely/entity-association-inspector-bundle",
    "description": "This bundle provide tool for inspection relations between entities.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrii Oprysko",
            "email": "anjey.link@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Corerely\\EntityAssociationInspectorBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Corerely\\EntityAssociationInspectorBundle\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.4",
        "symfony/http-kernel": "^7.0 || ^8.0",
        "symfony/dependency-injection": "^7.0 || ^8.0",
        "symfony/config": "^7.0 || ^8.0",
        "doctrine/orm": "^3.0",
        "doctrine/doctrine-bundle": "^2.15 || ^3.0",
        "doctrine/common": "^3.5"
    },
    "require-dev": {
        "symfony/framework-bundle": "^7.0 || ^8.0",
        "squizlabs/php_codesniffer": "^3.6",
        "slevomat/coding-standard": "^8.0",
        "phpunit/phpunit": "^12.1",
        "zenstruck/foundry": "^v2.7",
        "symfony/yaml": "^7.0 || ^8.0"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
